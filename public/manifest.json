{
  "name": "Bookmarks",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "manager for bookmarks",
  "permissions": ["storage", "tabs", "bookmarks"],
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
